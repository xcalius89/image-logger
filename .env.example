# .env.example for Tracker Project

# Secret used for tracking
TRACK_SECRET=your_track_secret_here

# The token for webhooks
HOOK_TOKEN=your_hook_token_here

# Public base URL
PUBLIC_BASE=https://example.com

# Discord webhook URL
DISCORD_WEBHOOK=https://discord.com/api/webhooks/example

# Redis database URL
REDIS_URL=redis://localhost:6379

# Path to Sherlock tool
SHERLOCK_PATH=/path/to/sherlock

# Other configuration variables
#...
